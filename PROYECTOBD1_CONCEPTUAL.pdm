FOR EACH ROW
BEGIN
    IF INSERTING THEN
	INSERT INTO BITACORAPROYECTO(ID_BITACORAPROYECTO, USUARIO, TABLA, DATO_ANTERIOR, DATO_NUEVO)
	VALUES(ID_BITACORA.NEXTVAL, USER, &#39;PERSONAPROYECTO&#39; , &#39;NUEVO USUARIO&#39;, :NEW.NOMBRE);
    END IF;
    IF DELETING THEN
    INSERT INTO BITACORAPROYECTO(ID_BITACORAPROYECTO, USUARIO, TABLA, DATO_ANTERIOR, DATO_NUEVO)
	VALUES(ID_BITACORA.NEXTVAL, USER, &#39;PERSONAPROYECTO&#39; , &#39;USUARIO BORRADO&#39;, :OLD.NOMBRE);
    END IF;
    IF UPDATING THEN 
    IF :NEW.NOMBRE &lt;&gt; :OLD.NOMBRE THEN
	INSERT INTO BITACORAPROYECTO(ID_BITACORAPROYECTO, USUARIO, TABLA, DATO_ANTERIOR, DATO_NUEVO)
	VALUES(ID_BITACORA.NEXTVAL, USER, &#39;PERSONAPROYECTO&#39; , :OLD.NOMBRE, :NEW.NOMBRE);
  END IF;
    IF :NEW.EDAD &lt;&gt; :OLD.EDAD THEN
    INSERT INTO BITACORAPROYECTO(ID_BITACORAPROYECTO, USUARIO, TABLA, DATO_ANTERIOR, DATO_NUEVO)
	VALUES(ID_BITACORA.NEXTVAL, USER, &#39;PERSONAPROYECTO&#39; , :OLD.EDAD, :NEW.EDAD);
      END IF;
    END IF;
END;

