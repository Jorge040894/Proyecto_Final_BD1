CREATE OR REPLACE TRIGGER TRG_AIUR_EMPLEADO
AFTER INSERT OR UPDATE ON EMPLEADO
REFERENCING OLD AS OLD
NEW AS NEW
FOR EACH ROW
BEGIN
    IF INSERTING THEN 
INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO ACCION, DATO_ANTERIOR, DATO_NUEVO)
VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;INSERCION&#39; , &#39;NUEVO USUARIO&#39;, :NEW.NOMBRE);
    END IF;
    IF UPDATING THEN
    IF :NEW.ID_EMPLEADO &lt;&gt; :OLD.ID_EMPLEADO
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;ID_EMPLEADO&#39;, &#39;ACTUALIZACION&#39;, :OLD.ID_EMPLEADO, :NEW.ID_EMPLEADO);
    END IF;
        IF :NEW.DPI &lt;&gt; :OLD.DPI
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;DPI&#39;, &#39;ACTUALIZACION&#39;, :OLD.DPI, :NEW.DPI);
    END IF;
        IF :NEW.NOMBRE &lt;&gt; :OLD.NOMBRE
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;NOMBRE&#39;, &#39;ACTUALIZACION&#39;, :OLD.NOMBRE, :NEW.NOMBRE);
    END IF;
        IF :NEW.APELLIDO &lt;&gt; :OLD.APELLIDO
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;APELLIDO&#39;, &#39;ACTUALIZACION&#39;, :OLD.APELLIDO, :NEW.APELLIDO);
    END IF;
        IF :NEW.EDAD &lt;&gt; :OLD.EDAD
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;EDAD&#39;, &#39;ACTUALIZACION&#39;, :OLD.EDAD, :NEW.EDAD);
    END IF;
        IF :NEW.ID_CORREO &lt;&gt; :OLD.CORREO
    INSERT INTO BITACORAPROYECTO1(ID_BITACORAPROYECTO, USUARIO, TABLA, CAMPO_MODIFICADO, ACCION, DATO_ANTERIOR, DATO_NUEVO)
 VALUES(ID_BITACORA.NEXTVAL, USER, &#39;EMPLEADO&#39;, &#39;CORREO&#39;, &#39;ACTUALIZACION&#39;, :OLD.CORREO, :NEW.CORREO);
